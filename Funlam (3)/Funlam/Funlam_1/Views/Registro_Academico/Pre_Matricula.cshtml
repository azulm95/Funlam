@model IEnumerable<Funlam_1.Models.curso>

@{
   
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.title</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.total_cupos)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cupos_actuales)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fecha_inicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fecha_fin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.horario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.materia.nombre_materia)
        </th>
        <th></th>
    </tr>
    @if ( Model != null ) {
@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.total_cupos)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cupos_actuales)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fecha_inicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fecha_fin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.horario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.materia.nombre_materia)
        </td>
       
    </tr>
    }
}
    
</table>
