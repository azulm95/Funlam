@model IEnumerable<Funlam_1.Models.deuda>

@{
    ViewBag.Title = "Deudas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Deudas</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.tipo_deuda)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.observacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.valor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.estado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fecha_deuda)
        </th>
      
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tipo_deuda)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.observacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.valor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.estado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fecha_deuda)
        </td>
    </tr>
}

</table>
